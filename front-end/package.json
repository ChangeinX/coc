    "idb": "^7.1.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@vitejs/plugin-react": "^4.0.0",
    "jsdom": "^26.1.0",
    "vite": "^7.0.4",
